{
  "name": "@ignavia/earl",
  "version": "1.2.1",
  "description": "A graph library.",
  "keywords": [
    "graph"
  ],
  "homepage": "http://ignavia.github.io/js-earl",
  "bugs": "https://github.com/Ignavia/js-earl/issues",
  "license": "MIT",
  "author": "Lars Reimann <mail@lars-reimann.eu> (http://lars-reimann.eu)",
  "files": [
    "dist/"
  ],
  "main": "dist/earl.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ignavia/js-earl.git"
  },
  "scripts": {
    "build": "jspm build ./src/earl.js - @ignavia/util - babel-regenerator-runtime  ./dist/earl.js --format cjs --skip-source-maps --minify",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot",
    "doc": "esdoc -c esdoc.json",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "@ignavia/util": "^1.1.7",
    "babel-regenerator-runtime": "^6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "esdoc": "^0.4.4",
    "isparta": "^4.0.0",
    "jspm": "^0.17.0-beta.6",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "jspmPackage": true,
  "registry": "npm",
  "format": "cjs",
  "jspm": {
    "name": "@ignavia/earl",
    "dependencies": {
      "@ignavia/util": "^1.1.7",
      "babel-regenerator-runtime": "^6.5.0"
    },
    "devDependencies": {
      "plugin-babel": "systemjs-plugin-babel@^0.0.2"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "core-js": "^1.2.0",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  }
}
